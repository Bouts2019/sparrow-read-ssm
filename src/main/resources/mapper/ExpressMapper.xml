<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.martini.sparrow.mapper.ExpressMapper">
    <select id="findAll" resultType="Express">
        select *
        from t_express;
    </select>
    <select id="findById" resultType="Express">
        select *
        from t_express where id = #{id};
    </select>
</mapper>